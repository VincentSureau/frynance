<!DOCTYPE html>

<title>Quote</title>

{% block body %}
    <h1>Quote</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ quote.id }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ quote.totalPrice }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ quote.title }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_quote_index') }}">back to list</a>

    <a href="{{ path('app_admin_quote_edit', {'id': quote.id}) }}">edit</a>

    {{ include('back/quote/_delete_form.html.twig') }}
{% endblock %}
